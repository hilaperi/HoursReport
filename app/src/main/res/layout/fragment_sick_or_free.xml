        tools:context=".ui.reports.SickOrFreeFragment">

        <ImageView
            android:id="@+id/statusImage"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

        </ImageView>

    </LinearLayout>

</LinearLayout>